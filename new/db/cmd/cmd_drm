NAME='drm xmm registers'
FILE=../bins/elf/sse2-add
ARGS=-d
CMDS=<<EOF
dcu main
4ds
drm xmm1 0 8
drm xmm1 15 8
drm xmm1 0 32
drm xmm1 3 32
drm xmm1 0 64
drm xmm1 1 64
drm xmm0
drm xmm1 * 8
drm xmm1 * 16
drm xmm1 * 32
drm xmm1 * 64
EOF
EXPECT=<<EOF
0x00000001
0x00000010
0x04030201
0x100f0e0d
0x807060504030201
0x100f0e0d0c0b0a09
